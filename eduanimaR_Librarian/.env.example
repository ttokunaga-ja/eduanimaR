# ─── Librarian サービス設定 ──────────────────────────────────────
# gRPC リスニングポート
LIBRARIAN_PORT=50051

# Gemini API キー（Google AI Studio で取得）
GEMINI_API_KEY=your-gemini-api-key-here

# LangGraph エージェント設定
# 最大推論ループ数（デフォルト: 3）
LIBRARIAN_MAX_LOOPS=3
# 1クエリあたりの最大検索結果数（デフォルト: 10）
LIBRARIAN_MAX_RESULTS=10
# タイムアウト（ミリ秒、デフォルト: 30000）
LIBRARIAN_TIMEOUT_MS=30000

# ロギング設定
LOG_LEVEL=INFO
